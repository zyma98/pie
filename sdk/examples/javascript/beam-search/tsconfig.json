{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "inferlet": ["../../inferlet-js/src/index.ts"],
      "inferlet/*": ["../../inferlet-js/src/*"]
    }
  },
  "include": ["*.ts", "../../inferlet-js/src/globals.d.ts"]
}
