{
  "name": "inferlet",
  "version": "0.1.0",
  "description": "JavaScript library for writing Pie inferlets",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:wasm": "vitest run test/wasm/__tests__",
    "generate-bindings": "jco types ../inferlet/wit -o src/bindings --world-name exec"
  },
  "dependencies": {
    "@huggingface/jinja": "^0.3.0"
  },
  "devDependencies": {
    "@bytecodealliance/jco": "^1.15.4",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0"
  }
}
